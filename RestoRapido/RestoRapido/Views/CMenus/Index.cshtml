@model IEnumerable<RestoRapido.Models.CMenu>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.m_strNom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.m_DateDebut)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.m_DateFin)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.m_strNom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.m_DateDebut)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.m_DateFin)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.m_iMenuId }) |
            @Html.ActionLink("Details", "Details", new { id=item.m_iMenuId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.m_iMenuId })
        </td>
    </tr>
}

</table>
