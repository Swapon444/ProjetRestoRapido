@using RestoRapido.Models
@model List<string>
@{
    ViewBag.Title = "Voici vos alertes";
}

<h2>@ViewBag.Title</h2>
<div class="row">
    
    <table>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.ToString()
                </td>
            </tr>
        }
    </table>

    @Html.ActionLink("Supprimer vos alertes", "Supprimer", "Serveur")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}